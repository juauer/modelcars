<?xml version="1.0"?>
<launch>
	<!-- arg <filename>: the bag is getting written to catkin_ws/../logs/<filename>.bag -->
	<arg name="filename" default="new" />
	
	<node name="log_recorder" pkg="rosbag" type="record" args="-O $(find logging)/../../../logs/$(arg filename) /odom /usb_cam/image_raw" output="screen" />
</launch>
